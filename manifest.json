{
  "name": "Mac Reload",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "http://eliran.net",
  "permissions": [
    "contentSettings"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://*.google.*/*"
      ],
      "js": [
        "js/jquery.min.js",
        "./inject.js"
      ]
    }
  ]
}